<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="car_reservation_action" model="ir.actions.act_window">
        <field name="name">Reservation</field>
        <field name="res_model">car.rent.reservation</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create a New Property Using CREATE Button!!
            </p>
        </field>
    </record>

    <record id="car_reservation_view_tree" model="ir.ui.view">
        <field name="name">Reservation</field>
        <field name="model">car.rent.reservation</field>
        <field name="arch" type="xml">
            <tree string="car_rent">
                
                    <field name="user_id"/>
                    <field name="rent_type"/>
            </tree>
        </field>
    </record>

    <record id="car_reservation_form" model="ir.ui.view">
        <field name="name">Car Reservation form</field>
        <field name="model">car.rent.reservation</field>
        <field name="arch" type="xml">
            <form string="car_reservation">
                
                <sheet>
                    <h1>Reservation</h1>
                    <group>
                        
                        <group>
                            <field name="user_id"/>
                        </group> 
                        <group>
                            <field name="rent_type"/>
                        </group>
                    </group>                  
                </sheet>
            </form>
        </field>
    </record>
</odoo>